
@{
    ViewBag.Title = "ผลแลป";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .containerIframe {
        position: relative;
        overflow: hidden;
        width: 100%;
        padding-top: 56.25%; /* 16:9 Aspect Ratio (divide 9 by 16 = 0.5625) */
    }

    /* Then style the iframe to fit in the container div with full height and width */
    .responsive-iframe {
        position: absolute;
        top: 0;
        left: 0;
        bottom: 0;
        right: 0;
        width: 100%;
        height: 100%;
    }
</style>

<div class="back-to-top"></div>


<div class="page-section">
    <div class="container">

        <div class="row">
            <div class="col-lg-8">
                <nav aria-label="Breadcrumb">
                    <ol class="breadcrumb bg-transparent py-0 mb-5">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">หน้าหลัก</a></li>
                        <li class="breadcrumb-item"><a href="@Url.Action("PatientLabSearch")">ตรวจสอบผลแลป</a></li>
                        <li class="breadcrumb-item"><a href="@Url.Action("LabResultDate")">ผลตรวจที่พบ</a></li>
                        <li class="breadcrumb-item active" aria-current="page">ผลแลป</li>
                    </ol>
                </nav>
            </div>
        </div> <!-- .row -->

        <div class="containerIframe">
            <iframe class="responsive-iframe" src="@ViewBag.urlPdf" style="width:1080px; height:720px;"></iframe>
        </div>
    </div>
</div>

